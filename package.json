{
  "name": "ubk",
  "version": "4.1.4",
  "description": "micro message broker",
  "main": "index.js",
  "scripts": {
    "coveralls": "npm install coveralls && node ./node_modules/coveralls/bin/coveralls.js < ./coverage/lcov.info",
    "discify": "browserify --ignore cnyks --node -p discify index.js | discify-reporter --ir://run --source=disc/graph.json",
    "test": "node node_modules/istanbul/lib/cli.js cover --report text-summary --report html --report lcov  node_modules/mocha/bin/_mocha",
    "jscs": "jscs --config=node_modules/ivs-jssc/ivs-node.jscsrc server client",
    "eslint": "eslint --config=node_modules/ivs-jssc/ivs-node.eslintrc.js server client",
    "checkall" : "npm run eslint && npm run jscs"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/131/ubk.git"
  },
  "keywords": [
    "message",
    "tcp",
    "ws"
  ],
  "author": "Francois Leurent <131.php@cloudyks.org>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/131/ubk/issues"
  },
  "homepage": "https://github.com/131/ubk",
  "dependencies": {
    "debug": "^2.3.0",
    "eventemitter-co": "^1.0.5",
    "ivs-jssc": "^2.1.1",
    "mout": "^1.0.0",
    "nyks": "^4.1.10"
  },
  "devDependencies": {
    "expect.js": "^0.3.1",
    "istanbul": "^0.4.5",
    "mocha": "^3.1.2",
    "ws": "^1.1.0"
  }
}